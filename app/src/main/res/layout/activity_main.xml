<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
		xmlns:android="http://schemas.android.com/apk/res/android"
		xmlns:tools="http://schemas.android.com/tools"
		xmlns:app="http://schemas.android.com/apk/res-auto"
		android:orientation="vertical"
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		tools:context=".main.MainActivity" android:layout_margin="48dp">

	<Button
			android:text="START GRAY CAMERA"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content" tools:layout_editor_absoluteY="102dp"
			tools:layout_editor_absoluteX="126dp" android:id="@+id/main_start_camera_button"
			android:layout_marginBottom="24dp"/>
	<Button
			android:text="START RECT CORRECTION"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:id="@+id/main_start_rectangle_button"/>
</LinearLayout>